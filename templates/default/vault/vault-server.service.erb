 [Unit]
 Description=Vault Server
 After=network.target

 [Service]
 User=root
 Group=root
 ExecStart=<%= @bin_vault %> server -config=<%= @etc_vault %>
 ExecReload=/bin/kill -HUP $MAINPID
 ExecStop=/bin/kill -TERM $MAINPID
 KillSignal=SIGINT
 Restart=on-failure

 [Install]
 WantedBy=multi-user.target
