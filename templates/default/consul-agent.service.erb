 [Unit]
 Description=Consul Server
 After=network.target

 [Service]
 User=consul
 Group=consul
 Environment="GOMAXPROCS=2"
 ExecStart=<%= @bin_consul %> agent -config-file <%= @etc_consul %>
 ExecReload=/bin/kill -9 $MAINPID
 KillSignal=SIGINT
 Restart=on-failure


 [Install]
 WantedBy=multi-user.target
